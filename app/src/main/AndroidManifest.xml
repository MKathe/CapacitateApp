<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cerezaconsulting.coreproject">


    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:name=".ApplicationExample"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity

            android:theme="@style/AppTheme"
            android:screenOrientation="portrait"
            android:name="com.cerezaconsulting.coreproject.presentation.activities.LoadActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity

            android:theme="@style/AppTheme"
            android:screenOrientation="portrait"
            android:name="com.cerezaconsulting.coreproject.presentation.activities.LoginActivity"></activity>

        <activity
            android:screenOrientation="portrait"
            android:name="com.cerezaconsulting.coreproject.presentation.activities.PanelActivity"></activity>
    </application>

</manifest>