<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cerezaconsulting.coreproject">


    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:name=".CompendioApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/indigoTheme">
        <activity
            android:name="com.cerezaconsulting.coreproject.presentation.activities.LoadActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name="com.cerezaconsulting.coreproject.presentation.activities.LoginActivity"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name="com.cerezaconsulting.coreproject.presentation.activities.PanelActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".presentation.activities.ChapterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.activities.QuestionActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>